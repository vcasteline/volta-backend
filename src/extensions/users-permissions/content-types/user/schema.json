{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "clasesDisponibles": {
      "type": "integer"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "user"
    },
    "nombre": {
      "type": "string",
      "regex": "^[A-Za-zÀ-ÖØ-öø-ÿ' -]+$",
      "minLength": 1,
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true,
      "regex": "^[A-Za-zÀ-ÖØ-öø-ÿ' -]+$",
      "minLength": 1
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "telefono": {
      "type": "string",
      "regex": "^\\+([0-9]{1,3}) ?([0-9]{1,4}) ?([0-9]{1,4}) ?([0-9]{1,9})$",
      "unique": false,
      "required": true
    },
    "past_bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::past-booking.past-booking",
      "mappedBy": "users_permissions_user"
    },
    "purchased_ride_packs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchased-ride-pack.purchased-ride-pack",
      "mappedBy": "user"
    }
  }
}
