{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "class": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::class.class"
    },
    "bicycles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bicycle.bicycle"
    },
    "bookingStatus": {
      "type": "enumeration",
      "enum": [
        "refunded",
        "completed",
        "cancelled"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "bookings"
    },
    "fechaHora": {
      "type": "datetime"
    },
    "guest": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::guest.guest",
      "mappedBy": "booking"
    }
  }
}
